<Window x:Class="KSVA2._0_WPF.Views.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KSVA2._0_WPF.Views"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Usernametxb" HorizontalAlignment="Center" Margin="0,99,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Passwordtxb" HorizontalAlignment="Center" Margin="0,122,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Phonetxb" HorizontalAlignment="Center" Margin="0,145,0,0" TextWrapping="Wrap" Text="Phone" VerticalAlignment="Top" Width="120"/>
        <DatePicker x:Name="Birthdtp" HorizontalAlignment="Center" Margin="0,193,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="birthLabel" Content="Date of birth" HorizontalAlignment="Center" Margin="0,163,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Registerbtn" Content="Register" HorizontalAlignment="Center" Margin="0,241,0,0" VerticalAlignment="Top" Height="25" Width="120" Click="Registerbtn_Click"/>
        <TextBox x:Name="Pricetxb" HorizontalAlignment="Left" Margin="465,145,0,0" TextWrapping="Wrap" Text="Price" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Subjecttxb" HorizontalAlignment="Left" Margin="465,122,0,0" TextWrapping="Wrap" Text="Subject" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="Expertisecbx" HorizontalAlignment="Left" Margin="465,95,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="high school"/>
            <ComboBoxItem Content="university"/>
            <ComboBoxItem Content="scientist"/>
        </ComboBox>
        <CheckBox x:Name="Teachercbx" Content="Teacher" HorizontalAlignment="Left" Margin="638,59,0,0" VerticalAlignment="Top" Checked="Teachercbx_Checked" Width="74" Unchecked="Teachercbx_Unchecked"/>

    </Grid>
</Window>
